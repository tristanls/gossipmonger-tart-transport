{
  "name": "gossipmonger-tart-transport",
  "version": "0.1.0",
  "description": "Tartjs transport for Gossipmonger",
  "scripts": {
    "assert-latest-node": "(node -v | grep -q ${npm_package_scriptvars_latest_node}) || (echo \"Expected node version ${npm_package_scriptvars_latest_node}, got $(node -v)\" && exit 1)",
    "test": "npm run assert-latest-node && npm list 1>/dev/null && node scripts/test.js"
  },
  "scriptvars": {
    "latest_node": "v7.1.0"
  },
  "main": "index.js",
  "devDependencies": {
    "nodeunit": "0.10.2",
    "scripts-nodeunit-test": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tristanls/gossipmonger-tart-transport.git"
  },
  "contributors": [
    "Tristan Slominski <tristan.slominski@gmail.com>"
  ],
  "license": "MIT"
}
